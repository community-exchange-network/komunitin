{
  "name": "komunitin",
  "version": "0.0.1",
  "description": "Open System for Exchange Communities",
  "productName": "Komunitin",
  "cordovaId": "org.komunitin.app",
  "capacitorId": "",
  "author": "komunitin <komunitinbox@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev -m pwa --env.USE_MIRAGE 200",
    "build": "quasar build -m pwa --env.USE_MIRAGE 200",
    "start": "quasar serve --history dist/pwa",
    "//": "Don't call `quasar test --unit jest` since it ends with wrong exit code.",
    "test": "jest",
    "test:coverage": "jest --silent --coverage src/",
    "test:watch": "jest --watch src/",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore --max-warnings 0 ./src",
    "doc": "typedoc --out docs --tsconfig tsconfig.json src",
    "todo": "egrep -r '@todo|@bug|@dev' src/",
    "testAll": "tsc -p tsconfig.json --noEmit && npm run test && npm run lint"
  },
  "dependencies": {
    "@alexlit/vue-yandex-share": "^1.0.1",
    "@electron/typescript-definitions": "^8.8.0",
    "@quasar/extras": "^1.9.13",
    "@types/clamp-js": "^0.7.2",
    "@types/dotenv": "^8.2.0",
    "@types/leaflet": "^1.5.19",
    "@types/marked": "^0.7.4",
    "@types/vuelidate": "^0.7.13",
    "axios": "^0.19.2",
    "clamp-js": "^0.7.0",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "leaflet": "^1.7.1",
    "markdown-to-txt": "^1.0.1",
    "marked": "^0.8.2",
    "node-sass": "^4.14.1",
    "quasar": "^1.15.0",
    "typedoc": "^0.17.8",
    "vue-element-loading": "^1.1.6",
    "vue-i18n": "^8.22.3",
    "vue2-leaflet": "^2.6.0",
    "vuelidate": "^0.7.6"
  },
  "devDependencies": {
    "@quasar/app": "^1.9.6",
    "@quasar/quasar-app-extension-dotenv": "^1.0.5",
    "@quasar/quasar-app-extension-testing": "^1.0.3",
    "@quasar/quasar-app-extension-testing-unit-jest": "^1.0.1",
    "@quasar/quasar-app-extension-typescript": "^1.0.2",
    "@types/faker": "^4.1.12",
    "@types/node": "^12.19.13",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "acorn": "^8.0.4",
    "devtron": "^1.4.0",
    "electron": "^8.5.5",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-vue": "^6.2.2",
    "eslint-plugin-vue-i18n": "^0.3.0",
    "faker": "^4.1.0",
    "jest": "^25.5.4",
    "kind-of": ">=6.0.3",
    "minimist": ">=1.2.3",
    "miragejs": "^0.1.41",
    "ts-jest": "^25.5.1",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7",
    "vue-jest": "^3.0.7"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "browserslist": [
