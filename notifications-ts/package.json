{
  "name": "notifications-ts",
  "version": "1.0.0",
  "description": "Komunitin Notifications Service (Node.js/TypeScript)",
  "type": "module",
  "main": "dist/index.mjs",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "dev": "tsx watch --inspect=0.0.0.0:9229 src/index.ts",
    "newsletter": "tsx --inspect=0.0.0.0:9229 src/scripts/newsletter.ts",
    "build": "pnpm typecheck && unbuild",
    "start": "node dist/index.mjs",
    "reset-db": "prisma migrate reset --force",
    "test": "node --experimental-test-module-mocks --env-file=.env.test --import tsx --test src/**/*.test.ts",
    "test-one": "node --experimental-test-module-mocks --env-file=.env.test --import tsx --test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@photostructure/tz-lookup": "^11.3.0",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "7.2.0",
    "bullmq": "^5.66.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.2.1",
    "express-oauth2-jwt-bearer": "^1.7.2",
    "handlebars": "^4.7.8",
    "i18next": "^25.7.3",
    "i18next-fs-backend": "^2.6.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.11",
    "pg": "^8.16.3",
    "pino": "^8.16.2",
    "redis": "^4.6.14",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@tsconfig/node22": "^22.0.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^20.10.4",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "jose": "^6.1.3",
    "msw": "^2.12.4",
    "pino-pretty": "^10.3.0",
    "prisma": "7.2.0",
    "supertest": "^7.2.2",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "unbuild": "^3.6.1"
  }
}