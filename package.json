{
  "name": "komunitin",
  "version": "0.0.1",
  "description": "Open System for Exchange Communities",
  "productName": "Komunitin",
  "author": "komunitin <komunitinbox@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env QENV=mirage quasar dev -m pwa",
    "dev-local": "cross-env QENV=local quasar dev -m pwa",
    "build-demo": "cross-env QENV=demo quasar build -m pwa",
    "build": "cross-env QENV=build quasar build -m pwa",
    "test": "quasar test --unit jest --dev=\"-m pwa\"",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore --max-warnings 0 ./src",
    "start": "quasar serve --history dist/pwa",
    "test:coverage": "jest --silent --coverage src/",
    "test:watch": "jest --watch src/",
    "doc": "typedoc --out docs --tsconfig tsconfig.json src",
    "testAll": "tsc -p tsconfig.json --noEmit && npm run test && npm run lint"
  },
  "dependencies": {
    "@quasar/extras": "^1.9.13",
    "@vue-leaflet/vue-leaflet": "^0.6.1",
    "@vuelidate/core": "^2.0.0",
    "@vuelidate/validators": "^2.0.0",
    "axios": "^0.21.1",
    "clamp-js": "^0.7.0",
    "core-js": "^3.8.3",
    "date-fns": "^2.16.1",
    "firebase": "^9.6.0",
    "leaflet": "^1.9.2",
    "markdown-to-txt": "^1.0.1",
    "marked": "^0.8.2",
    "quasar": "^2.9.2",
    "vue": "^3.2.40",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.5",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@electron/typescript-definitions": "^8.9.6",
    "@quasar/app-webpack": "^3.6.1",
    "@types/clamp-js": "^0.7.3",
    "@types/faker": "^4.1.12",
    "@types/jest": "^29.1.2",
    "@types/leaflet": "^1.8.0",
    "@types/marked": "^4.0.7",
    "@types/node": "^12.19.14",
    "@types/resize-observer-browser": "^0.1.5",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vue/test-utils": "^2.1.0",
    "@vue/vue3-jest": "^29.1.1",
    "acorn": "^8.0.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.8.0",
    "eslint-plugin-vue": "^9.6.0",
    "eslint-webpack-plugin": "^2.7.0",
    "faker": "^4.1.0",
    "jest": "^29.1.2",
    "kind-of": ">=6.0.3",
    "minimist": ">=1.2.3",
    "miragejs": "^0.1.41",
    "node-sass": "^7.0.3",
    "typedoc": "^0.17.8",
    "typescript": "~4.4",
    "workbox-webpack-plugin": "^6.0.2"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "browserslist": [
    "defaults"
  ]
}
